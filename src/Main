// The main method must be in a class named "Main".
class Main {
    public static void main(String[] args) {
        System.out.println("Hello world!");

        Cachorro cachorro = new Cachorro();
        cachorro.nome = "Puppy";
        cachorro.cor = "Marron";
        cachorro.altura = 25;
        cachorro.peso = 5.5;
        cachorro.tamanhoDoRabo = 5;

        System.out.println(cachorro);
    }
}
